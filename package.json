{
  "name": "bsproject-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start:dev": "npm-run-all -p watch:css start:js",
    "build": "npm-run-all build:*",
    "start": "npm i && vite",
    "build:js": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json,html}' --config .prettierrc",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,scss,css,md,json,html}' --config .prettierrc",
    "test": "echo Testing"
  },
  "dependencies": {
    "@types/jest": "^28.1.7",
    "@types/react-router-dom": "^5.3.3",
    "framer-motion": "^7.2.0",
    "hamburger-react": "^2.5.0",
    "jest": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  }
}
